pipeline {
    agent any 
    stages {
        stage('cloning') { 
            steps {
               echo "cloning"
                git branch: 'main', credentialsId: 'optidial', url: 'https://github.com/Oualitsen/optidial.git'
            }
        }
 stage('maven build') { 
            steps {
               echo "build"
                sh "mvn clean package"
            }
        }
 stage('Test') {
            steps {
                echo "unit tests"
                sh "mvn test"
            }
        }

    }
}
